{
  "private": true,
  "name": "noteyou",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "homepage": ".",
  "scripts": {
    "start-dev-web": "roadhog server",
    "start-dev-electron": "concurrently \"wait-on http://localhost:8000 && electron .\" \"npm run start-dev\"",
    "start-dev": "cross-env BROWSER=none roadhog server",
    "compile-render": "roadhog build && rm  -rf render_process && mv dist render_process ",
    "compile-main": "babel -D ./src/electron -d ./main_process",
    "electron-pack": "set ELECTRON_MIRROR=http://npm.taobao.org/mirrors/electron/ && rm  -rf build dist main_process render_process && npm run compile-render && npm run compile-main && electron-builder --dir",
    "electron-dist": "set ELECTRON_MIRROR=http://npm.taobao.org/mirrors/electron/ && rm  -rf build dist main_process render_process && npm run compile-render && npm run compile-main && electron-builder",
    "dev": "cross-env COMPRESS=none roadhog build --watch",
    "build": "roadhog build && rm  -rf render_process && mv build render_process ",
    "lint": "eslint --ext .js src test",
    "precommit": "npm run lint",
    "run-electron": "electron .",
    "start": "npm-run-all --parallel dev run-electron"
  },
  "dependencies": {
    "dva": "^2.4.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "electron-is-dev": "^1.2.0",
    "electron-updater": "^4.3.5",
    "electron-reload": "^1.5.0"
  },
  "devDependencies": {
    "antd": "^4.12.2",
    "babel-plugin-dva-hmr": "^0.3.2",
    "babel-plugin-import": "^1.13.3",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.3",
    "electron": "^11.2.3",
    "electron-builder": "^22.9.1",
    "eslint": "^4.14.0",
    "eslint-config-umi": "^0.1.1",
    "eslint-plugin-flowtype": "^2.34.1",
    "eslint-plugin-import": "^2.6.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "font-awesome": "^4.7.0",
    "history": "^5.0.0",
    "husky": "^0.12.0",
    "react-fontawesome": "^1.7.1",
    "redbox-react": "^1.4.3",
    "roadhog": "^2.5.0-beta.4",
    "wait-on": "^5.2.1"
  }
}
